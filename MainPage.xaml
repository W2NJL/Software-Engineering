<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="radioland4.MainPage" Title="Welcome to the Radioland app!">

    
    
    <StackLayout BackgroundColor="White" Padding="60" VerticalOptions="Center">


        <Image x:Name="myimage"></Image>

        <Label FontSize = "16" Text="Please select a demo location city." CharacterSpacing="2" />


        <Button Text="Tabernacle, NJ" BackgroundColor="NavajoWhite" Clicked="NavigateButton_OnClicked">
        </Button>
        <Button Text="Yonkers, NY" BackgroundColor="Goldenrod" Clicked="NYCButton_Clicked">
        </Button>
        <Button Text="Breezewood, PA" BackgroundColor="Salmon" Clicked="NavigateButton3_OnClicked">
        </Button>
    </StackLayout>
</ContentPage>  
